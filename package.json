{
  "private": true,
  "scripts": {
    "analyze": "cross-env ANALYZE=1 max build",
    "build": "max build",
    "deploy": "yarn run build && yarn run gh-pages",
    "dev": "yarn run start:dev",
    "gh-pages": "gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "postinstall": "max setup",
    "jest": "jest",
    "lint": "yarn run lint:js && yarn run lint:prettier && yarn run tsc",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src ",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier -c --write \"**/**.{js,jsx,tsx,ts,less,md,json}\" --end-of-line auto",
    "openapi": "max openapi",
    "prettier": "prettier -c --write \"**/**.{js,jsx,tsx,ts,less,md,json}\"",
    "preview": "yarn run build && max preview --port 8000",
    "record": "cross-env NODE_ENV=development REACT_APP_ENV=test max record --scene=login",
    "serve": "umi-serve",
    "start": "cross-env UMI_ENV=dev max dev",
    "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none UMI_ENV=dev max dev",
    "start:no-mock": "cross-env MOCK=none UMI_ENV=dev max dev",
    "start:pre": "cross-env REACT_APP_ENV=pre UMI_ENV=dev max dev",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none UMI_ENV=dev max dev",
    "test": "jest",
    "test:coverage": "yarn run jest -- --coverage",
    "test:update": "yarn run jest -- -u",
    "tsc": "tsc --noEmit"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": "yarn run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/icons": "latest",
    "@ant-design/pro-components": "latest",
    "@ant-design/use-emotion-css": "latest",
    "@umijs/route-utils": "latest",
    "antd": "latest",
    "classnames": "latest",
    "lodash": "latest",
    "moment": "latest",
    "omit.js": "latest",
    "rc-menu": "latest",
    "rc-util": "latest",
    "react": "latest",
    "react-dev-inspector": "latest",
    "react-dom": "latest",
    "react-helmet-async": "latest"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "latest",
    "@testing-library/react": "latest",
    "@types/classnames": "latest",
    "@types/express": "latest",
    "@types/history": "latest",
    "@types/jest": "latest",
    "@types/lodash": "latest",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "@types/react-helmet": "latest",
    "@umijs/lint": "latest",
    "@umijs/max": "latest",
    "cross-env": "latest",
    "eslint": "latest",
    "express": "latest",
    "gh-pages": "latest",
    "jest": "latest",
    "jest-environment-jsdom": "latest",
    "lint-staged": "latest",
    "mockjs": "latest",
    "prettier": "latest",
    "swagger-ui-dist": "latest",
    "ts-node": "latest",
    "typescript": "latest",
    "umi-presets-pro": "latest"
  }
}
